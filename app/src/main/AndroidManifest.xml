<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.mipt.feofanova.foodapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <!--
    <user-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <user-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/start_picture"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/start_picture"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ZeroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MealsListFragment" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".IngredientsInputFragment" />
        <activity android:name=".FavoriteFragment" />
        <activity android:name=".NavigationActivity"></activity>
    </application>

</manifest>